:3}},{"source":"ts/views/UserForm.ts","name":null,"original":{"line":63,"column":0},"generated":{"line":78,"column":0}},{"source":"ts/views/UserForm.ts","name":null,"original":{"line":63,"column":0},"generated":{"line":78,"column":7}},{"source":"ts/views/UserForm.ts","name":null,"original":{"line":63,"column":0},"generated":{"line":78,"column":8}},{"source":"ts/views/UserForm.ts","name":null,"original":{"line":63,"column":0},"generated":{"line":78,"column":15}},{"source":"ts/views/UserForm.ts","name":null,"original":{"line":63,"column":15},"generated":{"line":78,"column":18}},{"source":"ts/views/UserForm.ts","name":null,"original":{"line":63,"column":0},"generated":{"line":78,"column":26}}],"sources":{"ts/views/UserForm.ts":"import { EventsMapProps } from \"../ts-utils/interfaces\";\nimport { User } from \"../models\";\n\nclass UserForm {\n  constructor(public parent: Element, public model: User) {}\n\n  get eventsMap(): EventsMapProps {\n    return {\n      \"mouseover:h1\": this.onHeaderMouseover,\n      \"mouseout:h1\": this.onHeaderMouseout,\n      \"click:button\": this.onButtonClick,\n    };\n  }\n\n  onHeaderMouseover(): void {\n    console.log(\"Header Mouseover'ed!\");\n  }\n\n  onHeaderMouseout(): void {\n    console.log(\"Header Mouseout'ed!\");\n  }\n\n  onButtonClick(): void {\n    console.log(\"Button Click'ed!\");\n  }\n\n  bindEvents(fragment: DocumentFragment): void {\n    const { eventsMap } = this;\n    for (const eventKey in eventsMap) {\n      if (eventsMap.hasOwnProperty(eventKey)) {\n        const eventValue = eventsMap[eventKey];\n        const [eventName, selector] = eventKey.split(\":\");\n        const querySelectorFragments = element => {\n          element.addEventListener(eventName, eventValue);\n        };\n        fragment.querySelectorAll(selector).forEach(querySelectorFragments);\n      }\n    }\n  }\n\n  template(): string {\n    return `\n      <div>\n        <h1>User Form</h1>\n        <div>User Name: ${this.model.getAttr(\"name\")}</div>\n        <div>User Age: ${this.model.getAttr(\"age\")}</div>\n        <br />\n        <input />\n        <button>Click me</button>\n      </div>\n    `;\n  }\n\n  render(): void {\n    const templateElement = document.createElement(\"template\");\n    templateElement.innerHTML = this.template();\n    const fragment: DocumentFragment = templateElement.content;\n    this.bindEvents(fragment);\n    this.parent.append(fragment);\n  }\n}\n\nexport default UserForm;\n"},"lineCount":null}},"error":null,"hash":"be7ab13e065b7decfeea4997bdf7fa1c","cacheData":{"env":{}}}