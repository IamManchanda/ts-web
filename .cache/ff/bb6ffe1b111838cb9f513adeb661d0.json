, this.data);\n      } else {\n        await axios.post(\"http://localhost:3000/users\", this.data);\n      }\n    } catch (error) {\n      console.error(error);\n    }\n  }\n}\n\nexport default User;\n"},"lineCount":null}},"error":null,"hash":"35254cd83ff79e45120ba2f2cc70a509","cacheData":{"env":{}}}